{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style" : 0,
        "newline-per-chained-call": 0,
        "no-unused-vars": 0,
        "no-console": 0,
        "no-undef": 0,
        "new-cap": 0,
        "func-names": 0,
        "consistent-return": 0,
        "strict": 0,
        "no-new": 0,
        "no-underscore-dangle": 0
        
    }
}
