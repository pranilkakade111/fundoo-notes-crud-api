{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style" : 0,
        "newline-per-chained-call": 0,
        "no-unused-vars": 0,
        "no-console": 0
    }
}
